{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "CoinApi": {
    "RateLimitPerSecond": 10
  },

  "DataCollector": {
    "CollectionIntervalMinutes": 1,
    "MaxDegreeOfParallelism": 10,
    "OrderBookLimitLevels": 10000,
    "SymbolsRefreshIntervalHours": 24,
    "BatchSize": 50,
    "Exchanges": [ "BINANCE" ],
    "MarketTypes": [ "SPOT" ],
    "QuoteAssets": [ "USDT" ],
    "MinVolume24hUsd": 1000,
    "MaxSymbols": 20
  },

  "OrderBook": {
    "DepthLevels": [ 1.5, 3, 5, 8, 15, 30 ]
  }
}